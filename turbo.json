{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**"],
      "env": [
        "NEXT_PUBLIC_*",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_API_TOKEN",
        "R2_ACCESS_KEY_ID",
        "R2_SECRET_ACCESS_KEY",
        "R2_ACCOUNT_ID",
        "R2_BUCKET",
        "RPC_URL",
        "POOL_CONFIG_KEY"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"],
      "env": ["NODE_ENV"]
    },
    "clean": {
      "cache": false
    },
    "validate-env": {
      "cache": false,
      "outputs": []
    },
    "damm-v1-create-constant-product-pool": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-create-memecoin-pool": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-create-stable-pool": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-get-configs": {
