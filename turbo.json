      "env": ["RPC_URL"]
    },
    "damm-v1-create-position": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-withdraw-liquidity": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-claim-locked-fees": {
      "cache": false,
      "env": ["RPC_URL", "WALLET_PRIVATE_KEY"]
    },
    "damm-v1-get-locked-fees": {
      "cache": false,
      "env": ["RPC_URL"]
    },
    "damm-v2-get-position-fees": {
      "cache": false,
      "env": ["RPC_URL"]
    },
    "damm-v2-claim-position-fees": {
